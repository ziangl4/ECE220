#include <stdio.h>
// CREATE A FUNCTION TO CALCULATE THE NUMBER
void function(int rowï¼‰{
  int b=0;
  
  for(b=0;b<=row;b+=1){
     int c;
     int d=1
     for(c=1;c<=b;c+=1){
       d*=(row+1-c)/c
       }
       printf("%d",d);
     }
}
// THE MAIN FUNCTION
int main (){
  printf("Enter a row index")
  int d;
  scanf("%d",&d);
  //READ THE NUMBER FROM PEOPLE
  function(d);
  return 0;
  
}
